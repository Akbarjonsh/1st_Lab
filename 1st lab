            //LEVEL 3 
            //TASK 1

            #region 
            Double a = 0.1, h = 0.1, b = 1.0, x, num, s = 0;
            int i = 0, m = 1;
            const Double E = 0.0001;
            for (x = a; x <= b; x = x + h)
            {
                i = i + 1;
                while (true)
                {
                    num = Math.Pow((-1), i) * (Math.Pow(x, 2 * i)) / (2 * i) * m;
                    m = m * (m + 1);//для вычесления ф
                    s = s + num;
                    if (num < E)
                    {
                        Console.WriteLine(Math.Cos(x));

                    }
                    break;
                }
                #endregion


                //TASK 7
                #region
                int b = 1, i = 0;
                double a = 0.1, h = 0.05, x, s = 0, c, factor = 1, y, num, e = 2;
                const double E = 0.0001;
                for (x = a; x <= b; x = x + h)
                {
                    i = i + 1;
                    while (true)
                    {
                        factor = factor * (factor + 1);
                        num = (Math.Pow(x, 2 * i)) / ((2 * i) * factor);
                        s = s + num;
                        if (num < E)
                        {
                            y = ((Math.Pow(e, x)) + (Math.Pow(e, -x))) / 2;
                            Console.WriteLine(y);
                        }
                        break;
                    }
                }
                #endregion


