            days = 1;
            km = 10.0;
            for (days = 1; days <= 7; days += 1)
            {
                km = km + ((km / 100) * 10);
            }
            Console.WriteLine(km);
            #endregion

            //b
            #region
            int days;
            double km;
            days = 1;
            km = 10.0;
            while (km <= 100.0)
            {
                km = km + ((km / 100) * 10);
                days = days + 1;
            }
            Console.WriteLine(days);
            #endregion

