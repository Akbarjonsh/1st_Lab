    #region
            double x = Convert.ToDouble(Console.ReadLine());
            double s = 0, a;
            int n = 1, st = 1;
            for (n = 1; n < 9; n += 1)
            {
                for (st = 1; st < 8; st += 1)
                {
                    a = Math.Cos(x * n) / (x * st);
                    s = s + a;
                }
            }
            Console.WriteLine(s);
            #endregion

            #region
            int p = 1, n = 1;
            double s = 0;
            for (p = 1; p <= 6; p += 1)
            {
                s = s + ((-1) * (-1)) * (5 * 5) / (n * (n + 1));
            }
            Console.WriteLine(s);
            #endregion

            #region
            int a, b, c, d, m, k, p;
            a = 1; b = 1; c = 2; d = 1;
            k = 0; m = 0;
            p = 1;
            for (p = 3; p <= 5; p += 1)
            {
                m = b + d;
                d = m; b = d;
                k = a + c;
                a = c; c = k;
            }
            Console.WriteLine(k);//числитель
            Console.WriteLine(m);//знаменатель
            #endregion

            #region
            int a, b, c, d, m, k, p;
