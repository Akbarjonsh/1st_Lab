            //TASK 4
            #region
            double x, s, a, e;
            a = 0;
            e = 0.0001;
            s = 1;
            x = Convert.ToDouble(Console.ReadLine());
            x = Math.Abs(x);
            if (x < 1)
            {
                while (a > e)
                {
                    a = x * x * x;
                    s = s + a;
                }
            }
            Console.WriteLine(s);
